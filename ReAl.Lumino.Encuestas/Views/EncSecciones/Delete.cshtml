@model ReAl.Lumino.Encuestas.Models.EncSecciones

@section Styles
{
	<!-- Bootstrap-Table CSS -->
	<link href="~/lib/bootstrap-table/dist/bootstrap-table.min.css" rel="stylesheet">
	<!-- Sweetalert CSS -->
	<link href="~/lib/sweetalert2/dist/sweetalert2.min.css" rel="stylesheet">
}	
	
@{
    ViewData["Title"] = "Delete";
}

<div class="row">
	<div class="col-lg-12">
		<h1 class="page-header">EncSecciones</h1>
	</div>
</div><!--/.row-->

<div class="row">
<div class="col-lg-12">
<div class="panel panel-default">
<div class="panel-heading">¿Est&aacute; seguro que desea ELIMINAR &eacute;ste registro?</div>
<div class="panel-body">
    <div class="col-md-6">
        <div class="form-group">
            <a title="Volver al listado" class="btn btn-primary" asp-action="Index" ><i class="fa fa-arrow-left"></i></a>
            <a title="Eliminar registro" class="btn btn-danger" onclick="Validate(this, event);"><i class="fa fa-trash-o"></i></a>
        </div>    
        <div class="form-group">
            <dl class="dl-horizontal">
                <dt>
                    @Html.DisplayNameFor(model => model.Codigo)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.Codigo)
                </dd>
                <dt>
                    @Html.DisplayNameFor(model => model.Seccion)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.Seccion)
                </dd>
                <dt>
                    @Html.DisplayNameFor(model => model.Abierta)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.Abierta)
                </dd>
                <dt>
                    @Html.DisplayNameFor(model => model.Apiestado)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.Apiestado)
                </dd>
                <dt>
                    @Html.DisplayNameFor(model => model.Apitransaccion)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.Apitransaccion)
                </dd>
                <dt>
                    @Html.DisplayNameFor(model => model.Usucre)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.Usucre)
                </dd>
                <dt>
                    @Html.DisplayNameFor(model => model.Feccre)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.Feccre)
                </dd>
                <dt>
                    @Html.DisplayNameFor(model => model.Usumod)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.Usumod)
                </dd>
                <dt>
                    @Html.DisplayNameFor(model => model.Fecmod)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.Fecmod)
                </dd>
                <dt>
                    @Html.DisplayNameFor(model => model.IdcnvNavigation)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.IdcnvNavigation.Apiestado)
                </dd>
                <dt>
                    @Html.DisplayNameFor(model => model.IdopyNavigation)
                </dt>
                <dd>
                    @Html.DisplayFor(model => model.IdopyNavigation.Apiestado)
                </dd>
            </dl>    
            <form asp-action="Delete" role="form" method="post" id="DeleteForm" class="form-horizontal row-border">
                <input type="hidden" asp-for="Idese" />
            </form>
        </div>
    </div>
</div>
</div>
</div>
</div><!-- /.row -->

@section Scripts
{
	<!-- Sweetalert JavaScript -->
	<script src="~/lib/sweetalert2/dist/sweetalert2.min.js"></script>
	
	<script>
		function Validate(ctl, event)
		{
			event.preventDefault();
			swal({
				title: "¿Seguro que desea ELIMINAR el registro?",
				text: "Verifique la información antes de aceptar!",
				type: "question",
				showCancelButton: true,
				confirmButtonColor: "#DD6B55",
				confirmButtonText: "Eliminar",
				cancelButtonText: "Cancelar"
			}).then(function(isConfirm) {
				if (isConfirm.value) {
					$("#DeleteForm").submit();
				} else {
					swal("Cancelled", "Se ha cancelado la accion", "error");
				}
			});
		}
	</script>
}	

